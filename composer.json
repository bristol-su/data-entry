{
    "name": "bristol-su/data-entry",
    "description": "A module for the Bristol SU Portal Project to allow entering and management of data in a given data structure",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Toby Twigger",
            "email": "tobytwigger1@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "bristol-su/support": "dev-portal-v4 as 4.0",
        "league/csv": "^9.6"
    },
    "require-dev": {
        "orchestra/testbench": "^6.19",
        "laravel/legacy-factories": "^1.1",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "BristolSU\\Module\\DataEntry\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BristolSU\\Module\\Tests\\DataEntry\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BristolSU\\Module\\DataEntry\\ModuleServiceProvider"
            ]
        }
    }
}
